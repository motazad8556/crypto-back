      "ts-node": "3.3.0",
      "typescript": "^3.4.5"
   },
   "dependencies": {
      "bcrypt": "^3.0.6",
      "body-parser": "^1.18.1",
      "dotenv": "^8.0.0",
      "events": "^3.0.0",
      "express": "^4.15.4",
