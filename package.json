   "scripts": {
      "start": "node build/index.js",
      "nodemon": "nodemon src/index.ts",
      "start_ts": "ts-node src/index.ts",
      "test": "mocha -r ts-node/register src/test/*.spec.ts --exit",
      "dist": "npm run build",
      "build": "tsc",
      "copy_dist": "npm run _refresh_distFolder && npm run _copyDist && npm run _copy_modules",
      "_refresh_distFolder": "rm -rf ../node_dist/*",
      "_copyDist": "cp -r ./build/* ../node_dist && cp ./package.json ../node_dist",
      "_copyConfigs": "npm run _copyConfig_ormConfig",
      "_copyConfig_ormConfig": "cp ./ormconfig.prod.json ../node_dist && mv ../node_dist/ormconfig.prod.json ../node_dist/ormconfig.json",
      "_copy_modules": "cp -r ./node_modules ../node_dist"
   }
